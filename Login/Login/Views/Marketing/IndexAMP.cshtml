    <div class="col-12 estiloImg">
        <a href="~/marketing/webinarEducacionAmp" target="_blank"><amp-img src="~/Content/landing main webinar/pagina_webinarsPasados_educacion1.png" width="600" height="600" layout="responsive"></amp-img></a>
    </div>
    <div class="col-12 estiloImg">
        <a href="~/marketing/webinarMunicipioAMP" target="_blank"><amp-img src="~/Content/landing main webinar/Municipio/pagina_webinarsPasados_MUNICIPIO.png" width="600" height="600" layout="responsive"></amp-img></a>
    </div>
    <div class="col-12 estiloImg">
        <a href="~/marketing/webinarAgricultura" target="_blank"><amp-img src="~/Content/landing main webinar/pagina_webinarsPasados_agricultura.png" width="600" height="600" layout="responsive"></amp-img></a>
    </div>
    <div class="col-12 estiloImg">
        <a href="~/marketing/webinarIncendio" target="_blank"><amp-img src="~/Content/landing main webinar/pagina_webinarsPasados_incendios.png" width="600" height="600" layout="responsive"></amp-img></a>
    </div>
    <div class="col-12 estiloImg">
        <a href="~/marketing/webinarGenero" target="_blank"><amp-img src="~/Content/imagennes reemplazar webinar master/pagina_webinarsPasados_violenciamujer.png" width="600" height="600" layout="responsive"></amp-img></a>
    </div>
    <div class="col-12 estiloImg">
        <a href="~/marketing/webinarAgenciaDIAMP" target="_blank"><amp-img src="~/Content/landing main webinar/pagina_webinarsPasados_agenciaDI.png" width="600" height="600" layout="responsive"></amp-img></a>
    </div>
    <div class="col-12 estiloImg">
        <a href="~/marketing/webinarGlaciares" target="_blank"><amp-img src="~/Content/landing main webinar/pagina_webinarsPasados_glaciares.png" width="600" height="600" layout="responsive"></amp-img></a>
