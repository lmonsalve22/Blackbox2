
<div class="row " >
    <div class="col-8">
        <h3 class="text-dark mb-0">Informes Interactivos</h3>
        <hr style="border:0.5px solid #000000" />
    </div>
</div>

<div class="row">
    <div class="col">
        @*<P><strong> Se muestran <span>@ViewBag.Resultado.Count </span>resultados de Informes Interactivos</strong></P>*@
    </div>
</div>
<div class="row responsive" style="overflow:scroll;height:350px;overflow-x:hidden; width:67%;">
    @if (ViewBag.InformeInteractivo == null)
    {
        <div class="col-8">
            <div class="row ">
                <div class="col-9 text-dark font-weight-bold mb-0" style="color:#518bb3 !important;cursor: pointer;">
                    No existen Informes interactivos.
                </div>
            </div>
        </div>
    }
    else
    {
        foreach (var item in ViewBag.InformeInteractivo)

        {
            <div class="col-8">
                <div class="row ">
                    <div class="col-2">
                        <img class="img-fluid" src="~/Content/image_UsuarioIndex/icono_informeinteractivo_AZUL.png" />
                    </div>
                    <div class="col-9 text-dark font-weight-bold mb-0" style="color:#518bb3 !important;cursor: pointer;">
                        <a onclick="mostrador('#catorce'); InforReporSuscripcion('@item.url', '@item.nombre');">
                            @item.nombre <br />
                            @item.TEMPORALIDAD.nombre
                        </a>
                    </div>
                </div>
            </div>



        }
    }


</div>
