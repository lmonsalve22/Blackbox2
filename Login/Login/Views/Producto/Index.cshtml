
@{
    ViewBag.Title = "Index";
}


<h1 class="">Hola @ViewBag.User, esperamos poder ayudarte</h1>

<br />
<h2>Estos son tus productos:</h2>
<table class="table table-bordered table-dark" align="center">
    <thead class="thead-dark">
        <tr>
            <th> Nombre </th>
            <th> Producto </th>
            <th> Descripcion </th>
            <th> Comprobante </th>
            <th> SKU </th>
            <th> Product ID </th>
            <th> Fecha </th>
            <th> Imagen </th>
            <th> Dias </th>
            <th>Descuentos</th>
            @*<th> JSON </th>*@
           
        </tr>
    </thead>
    @foreach (var item in ViewBag.Resultado)
    {
        <tbody>
            <tr>
                <th>
                    @item.NOMBRE
                </th>

                <td>
                    <a href="~/Pagina/ValidadorProductosInternos/@item.ID" style="color:lightblue;">
                        @item.ID
                    </a>
                </td>
                <td>
                    @{ 
                        IHtmlString str = new HtmlString(item.DESCRIPCION);
                    }
                    @Html.Raw(str)

                    @*@item.DESCRIPCION*@
                </td>

                <td>
                    <a href="@item.COMPROBANTE" target="_blank" style="color:lightblue;">
                        @item.COMPROBANTE
                    </a>
                </td>
                <td>

                    @item.SKU

                </td>
                <td>
                    @item.PRODUCT_ID
                </td>
                <td>
                    @item.FECHA_CREADO2;
                </td>
                <td>
                    <img src="@item.URL_IMAGEN" alt="Girl in a jacket" width="500" height="600">


                </td>
                <td>
                    @item.DIAS_DESDE_COMPRA
                </td>
                <td>
                    Este
                    @item.CODIGO_DESCUENTO
                </td>
            </tr>
        </tbody>
    }

</table>

