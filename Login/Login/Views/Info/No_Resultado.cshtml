    <script>
        function mostrador(seccion) {
            var arreglo = [
                '#PBuno',
                '#PBdos',
                '#PBcuatro',
                '#PBcinco'
            ];
            //arreglo.forEach
            jQuery.each(arreglo, function (i, val) {
                $(val).hide();
            });
            $(seccion).show();

        }
        function mostrador2(seccion1, seccion2, seccion3) {
            var arreglo = [
                '#PBuno',
                '#PBdos',
                '#PBcuatro',
                '#PBcinco'
            ];
            //arreglo.forEach
            jQuery.each(arreglo, function (i, val) {
                $(val).hide();
            });
            $(seccion1).show();
            $(seccion2).show();
            $(seccion3).show();
        }
    </script>
