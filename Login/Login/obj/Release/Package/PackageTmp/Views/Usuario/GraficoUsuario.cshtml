@*Productos Datas*@




@*DUDA*@



    @*Productos Datas*@
    <div class="container-fluid offset-1">
        <div class="row ">
            <div class="col-8">
                <h3 class="text-dark mb-0">Plataformas</h3>
                <hr style="border:0.5px solid #000000" />
            </div>
        </div>

        <div class="row">
            <div class="col">
                @*<P><strong> Se muestran <span>@ViewBag.Resultado.Count </span>resultados de Repotes 360°</strong></P>*@
            </div>
        </div>
        <div class="responsive row " style="overflow:scroll;height:350px;overflow-x:hidden; width:67%;">
            @foreach (var item in ViewBag.Resultado)

            {
                <div class="col-8">
                    <div class="row ">
                        <div class="col-2">
                            <img class="img-fluid mb-3" src="@item.URL_IMAGEN">
                        </div>
                        <div class="col-9 text-dark font-weight-bold mb-0" style="color:#518bb3 !important;cursor: pointer;">
                            <a onclick="mostrador('#doce'); ProductoSuscripcion('@item.SKU', '@item.NOMBRE');">
                                @item.NOMBRE <br />
                                @item.FECHA_CREADO
                            </a>
                        </div>
                    </div>
                </div>

            }
        </div>
    </div>
