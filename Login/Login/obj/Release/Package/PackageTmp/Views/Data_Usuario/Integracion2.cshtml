    <script>
        function mostrador(seccion1) {
            var arreglo = [
                '#in',
                '#in1',
                '#in2'
            ];
            //arreglo.forEach
            jQuery.each(arreglo, function (i, val) {
                $(val).hide();
            });
            $(seccion1).show();
        }
    </script>

    <script>
        function ActivarMenuSuperior(opcionMenu) {
            $('.active').removeClass('active');
            $(opcionMenu).addClass("active");
        }
    </script>
