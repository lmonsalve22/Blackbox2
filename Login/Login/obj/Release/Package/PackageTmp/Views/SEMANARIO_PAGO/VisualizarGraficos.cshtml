@{
    ViewBag.Title = "VisualizarGraficos";
    Layout = "~/Views/Shared/_LayoutSEMANARIOPAGO2.cshtml";
}



@*seccion iframe grafico*@
<section class="container-fluid contenedor row mr-md-2 justify-content-center mb-5">
    <main class="col-11 col-md-10 mt-2 " style="border:1px solid #e6e6e6; border-radius:10px; max-height:1060px !important;">
        <!--Iframe de grafico-->
        <div>
            @*<iframe id="Imprimeme" class="pruebarara" style="width:100%; height:490px; margin-top:5% !important;" src="@ViewBag.Elemento.url">*@
            <iframe id="Imprimeme" class="pruebarara" style="width:100%; height:920px; margin-top:5% !important;" src="@ViewBag.grafico.url">
            </iframe>
        </div>
        <div class="row quitarespacio2 justify-content-center" style="color:blue; font-size:10px;">
            @*Logo de DATA INTELLIGENCE PIE DEL IFRAME*@
            <div class="col-12 col-md-12">
                <div class="row justify-content-center">
                    <div class="col-12 col-md-5 mb-1 p-1">
                        <img class="img-fluid " src="~/Content/img/logosDataIntelligence_creadopor.png" alt="Logo transparente" />
                    </div>
                </div>
            </div>
        </div>
    </main>
</section>